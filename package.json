{
	"name": "8f4e",
	"version": "1.0.0",
	"description": "8f4e",
	"scripts": {
		"build": "parcel build src/index.html && cp ./src/_headers ./dist/_headers",
		"dev": "parcel src/index.html --port 3000 --hmr-port 30000",
		"sprite": "parcel packages/sprite-generator/visual-testing/index.html --port 3001 --hmr-port 30001 --cache-dir .parcel-cache-sprite --dist-dir dist-sprite",
		"prettier": "prettier --write './src/**/*.ts' && prettier --write './packages/**/*.ts'",
		"eslint": "npx eslint . --ext .ts --fix",
		"test": "node --experimental-vm-modules --experimental-wasi-unstable-preview1 node_modules/.bin/jest",
		"prepare": "husky install"
	},
	"browserslist": [
		"last 1 Chrome version"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/andormade/8f4e.git"
	},
	"author": "Andor Polgar",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/andormade/8f4e/issues"
	},
	"homepage": "https://github.com/andormade/8f4e#readme",
	"devDependencies": {
		"@types/jest": "27.0.3",
		"@types/jest-environment-puppeteer": "5.0.0",
		"@types/jest-image-snapshot": "4.3.1",
		"@types/puppeteer": "5.4.4",
		"@typescript-eslint/eslint-plugin": "5.8.1",
		"@typescript-eslint/parser": "5.8.1",
		"eslint": "8.5.0",
		"eslint-config-prettier": "8.3.0",
		"glslify-bundle": "5.1.1",
		"glslify-deps": "1.3.2",
		"husky": "7.0.4",
		"jest": "27.4.5",
		"jest-image-snapshot": "4.5.1",
		"jest-puppeteer": "6.0.3",
		"lint-staged": "12.1.4",
		"parcel": "2.0.1",
		"prettier": "2.5.1",
		"puppeteer": "9.1.1",
		"ts-jest": "27.1.2",
		"typescript": "4.5.4"
	},
	"alias": {
		"bytecode-utils": "./packages/bytecode-utils/src",
		"compiler": "./packages/compiler/src",
		"2d-engine": "./packages/2d-engine/src",
		"sprite-generator": "./packages/sprite-generator/src",
		"worker": "./packages/worker/src"
	},
	"lint-staged": {
		"*.ts": "eslint --cache --fix",
		"*.{ts,css,md}": "prettier --write"
	},
	"dependencies": {
		"buffer": "5.7.1"
	}
