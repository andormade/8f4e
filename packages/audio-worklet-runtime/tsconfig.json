{
	"compilerOptions": {
		"target": "esnext",
		"moduleResolution": "node",
		"lib": ["esnext", "webworker"],
		"downlevelIteration": true,
		"esModuleInterop": true,
		"outFile": "dist/index.js",
		"declaration": true,
		"skipLibCheck": true,
		"strictNullChecks": true,
		"module": "system",
        "types": ["audioworklet"]
	},
	"include": ["./src/**/*.ts"],
	"exclude": ["node_modules"]
}
