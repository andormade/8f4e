{
	"compilerOptions": {
		"target": "esnext",
		"moduleResolution": "node",
		"lib": ["esnext", "webworker"],
		"module": "commonjs",
		"downlevelIteration": true,
		"esModuleInterop": true,
		"outDir": "dist/",
		"declaration": true,
		"skipLibCheck": true,
		"strictNullChecks": true
	},
	"include": ["./src/**/*.ts"],
	"exclude": ["node_modules"]
}
